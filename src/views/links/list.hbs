<div class="container p-4">
    <div class="row">
        {{#each links}}
            <div class="col-md-4">
                <div class="card text-cente m-2">
                    <div class="card-body">
                        <a href="{{url}}" target="_blank"><h3 class="card-title text-uppercase">{{title}}</h3></a>
                        <p class="m-2">{{description}}</p>
                        <p>{{time created_at}}</p>
                        <div class="container ">
                            <a href="" class="btn btn-danger mr-2">Eliminar</a>
                        <a href="" class="btn btn-info">Editar</a>
                        </div>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
</div>